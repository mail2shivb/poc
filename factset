openapi: 3.1.0
info:
  title: FactSet APIs – Unified Umbrella
  version: "1.0.0"
  description: |
    Single entry-point that organizes FactSet APIs and is intended to be merged
    with the official per-API OpenAPI specs you download from the FactSet Developer
    portal. Many specs require authentication to access. See externalDocs for catalog.
  termsOfService: https://developer.factset.com/
  contact:
    name: FactSet Developer Portal
    url: https://developer.factset.com/
externalDocs:
  description: FactSet API Catalog
  url: https://developer.factset.com/api-catalog

servers:
  - url: https://api.factset.com
    description: Production (example root; individual APIs may use different subpaths/hosts)

tags:
  # Content / Reference Data
  - name: Fundamentals
    description: FactSet Fundamentals API
    externalDocs: { url: https://developer.factset.com/api-catalog/factset-fundamentals-api }
  - name: Estimates
    description: FactSet Estimates API
    externalDocs: { url: https://developer.factset.com/api-catalog/factset-estimates-api }
  - name: Benchmarks
    description: Benchmarks & index data
    externalDocs: { url: https://developer.factset.com/api-catalog/factset-benchmarks-api }
  - name: Prices
    description: End-of-day & intraday prices (equities, FI)
    externalDocs: { url: https://developer.factset.com/api-catalog/factset-prices-api }
  - name: GlobalPrices
    description: Global prices coverage
    externalDocs: { url: https://developer.factset.com/api-catalog/factset-global-prices-api }
  - name: Ownership
    description: Fund/holder ownership
    externalDocs: { url: https://developer.factset.com/api-catalog/factset-ownership-api }
  - name: MergersAndAcquisitions
    description: Corporate M&A data
    externalDocs: { url: https://developer.factset.com/api-catalog/factset-mergers-and-acquisitions-api }
  - name: PrivateCompany
    description: Private company data
    externalDocs: { url: https://developer.factset.com/api-catalog/factset-private-company-api }
  - name: Entity
    description: Entity master & identifiers
    externalDocs: { url: https://developer.factset.com/api-catalog/factset-entity-api }

  # Documents / News
  - name: Documents
    description: Documents Distributor – filings, news, StreetAccount, etc.
    externalDocs: { url: https://developer.factset.com/api-catalog/documents-distributor-documents-api }

  # Real-time / Market Data Models
  - name: ExchangeDatafeedSnapshot
    description: Consolidated real-time/delayed snapshot
    externalDocs: { url: https://developer.factset.com/api-catalog/exchange-datafeed-snapshot-api-symbol-list }
  - name: ExchangeDatafeedDataModel
    description: Mapping tables / enumerations for real-time products
    externalDocs: { url: https://developer.factset.com/api-catalog/exchange-datafeed-data-model-api }

  # Utilities / Calculations
  - name: Formula
    description: FactSet Formula API
    externalDocs: { url: https://developer.factset.com/api-catalog/formula-api }

  # Marketplace / Partner
  - name: OpenFactSetMarketplace
    description: Open:FactSet Marketplace catalog access
    externalDocs: { url: https://developer.factset.com/api-catalog/openfactset-marketplace-api }
  - name: OpenFactSetPartnersDocuments
    description: Partner documents API
    externalDocs: { url: https://developer.factset.com/api-catalog/openfactset-partners-documents }

paths: {}
  # Intentionally empty. You will merge official per-API specs here.
  # After merging, this section will be populated with all endpoints.

components:
